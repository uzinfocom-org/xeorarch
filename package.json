{
    "name": "xeorarch",
    "version": "1.9.0",
    "description": "Xinux jamiyati uchun yozilgan Archlinux SDK",
    "main": "lib/mod.js",
    "types": "lib/mod.d.ts",
    "scripts": {
        "cache": "deno cache src/mod.ts",
        "format": "deno fmt --config deno.json",
        "format:check": "deno fmt --check --config deno.json",
        "lint": "deno lint --config deno.json",
        "test": "deno test --allow-all",
        "prepare": "npm run backport",
        "backport": "deno2node tsconfig.json",
        "postversion": "git push && git push --tags"
    },
    "files": [
        "lib/**/*"
    ],
    "keywords": [
        "archlinux",
        "packages",
        "repository",
        "xinux"
    ],
    "author": "UwUssimo Robinson <uwussimo@icloud.com> (https://uwussi.moe)",
    "license": " Apache-2.0",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/xinuxuz/xeorarch.git"
    },
    "bugs": {
        "url": "https://github.com/xinuxuz/xeorarch/issues"
    },
    "homepage": "https://github.com/xinuxuz/xeorarch#readme",
    "devDependencies": {
        "@types/node": "^12.20.47",
        "@types/node-fetch": "^2.6.1",
        "deno2node": "^1.2.1"
    },
    "dependencies": {
        "cheerio": "^1.0.0-rc.10",
        "fuse.js": "^6.5.3",
        "node-fetch": "^2.6.7"
    }
}
