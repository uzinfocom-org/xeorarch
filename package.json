{
  "name": "xeorarch",
  "version": "1.6.0",
  "description": "Xinux jamiyati uchun yozilgan Archlinux SDK",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "docs": "typedoc --theme github-wiki",
    "build": "tsc",
    "watch": "tsc -w",
    "format": "prettier --write --check \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "archlinux",
    "packages",
    "repository",
    "xinux"
  ],
  "author": "UwUssimo Robinson <uwussimo@icloud.com> (https://uwussi.moe)",
  "license": " Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xinuxuz/xeorarch.git"
  },
  "bugs": {
    "url": "https://github.com/xinuxuz/xeorarch/issues"
  },
  "homepage": "https://github.com/xinuxuz/xeorarch#readme",
  "devDependencies": {
    "@types/jest": "^27.4.0",
    "jest": "^27.4.7",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typedoc": "^0.22.11",
    "typedoc-github-wiki-theme": "^1.0.0",
    "typedoc-plugin-markdown": "^3.11.12",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "axios": "^0.25.0",
    "fuse.js": "^6.5.3"
  }
}
